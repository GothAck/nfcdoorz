namespace nfcdoorz.ipc.policy;

table TestCall {}

table TestReply {}

table RequestEvent {
  deviceID: string;
  cardUID: [uint8];
}

union Calls { TestCall }
union Replies { TestReply }
union Events { RequestEvent }

table Call {
  id: uint64;
  msg: Calls;
}

table Reply {
  id: uint64;
  msg: Replies;
  event: Events;
}

table Server {
  call: Call;
  reply: Reply;
}
